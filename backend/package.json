{
  "name": "vibes-marketplace-backend",
  "version": "1.0.0",
  "description": "Backend API for Vibes Marketplace",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
  "dev:mongo": "cross-env USE_MONGODB=true ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "seed": "ts-node src/scripts/seed.ts",
  "seed:mongo": "cross-env USE_MONGODB=true ts-node src/scripts/seed.ts",
    "clean": "rimraf dist/",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "@types/mongodb": "^4.0.6",
    "@types/mongoose": "^5.11.96",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
  "cross-env": "^7.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
